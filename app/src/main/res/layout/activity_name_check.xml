<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".nameCheck">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/ebisuButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="59dp"
        android:background="#FFAB00"
        android:fontFamily="sans-serif-black"
        android:text="恵美酒宮天満神社"
        android:textColor="#000000"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/hamanomiyaButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="40dp"
        android:background="#FFD600"
        android:fontFamily="sans-serif-black"
        android:text="浜の宮天満宮"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ebisuButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/matubaraButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="40dp"
        android:background="#DD2C00"
        android:fontFamily="sans-serif-black"
        android:text="松原八幡神社"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hamanomiyaButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/oosioButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="40dp"
        android:background="#64DD17"
        android:fontFamily="sans-serif-black"
        android:text="大塩天満宮"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/matubaraButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/arakawaButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="40dp"
        android:background="#FF00E5"
        android:fontFamily="sans-serif-black"
        android:text="荒川神社"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/oosioButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/usukiButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="40dp"
        android:background="#00B8D4"
        android:fontFamily="sans-serif-black"
        android:text="魚吹八幡神社"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/arakawaButton" />

</androidx.constraintlayout.widget.ConstraintLayout>