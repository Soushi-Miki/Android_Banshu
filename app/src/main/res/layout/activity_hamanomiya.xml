<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EFE29F"
    tools:context=".Hamanomiya">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:fontFamily="sans-serif-black"
        android:text="屋台蔵の場所を確認する"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/sukaButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="40dp"
        android:background="#0091EA"
        android:fontFamily="serif"
        android:text="須加"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/miyaButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="40dp"
        android:background="#E32A2A"
        android:fontFamily="serif"
        android:text="宮"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/sukaButton"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/tensinButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="40dp"
        android:background="#FF89BE"
        android:fontFamily="serif"
        android:text="天神"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/miyaButton"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/oohamaButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:background="#00B8D4"
        android:fontFamily="serif"
        android:text="大浜"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sukaButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/kawatigosoeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:background="#64DD17"
        android:fontFamily="serif"
        android:text="川内細江"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/oohamaButton"
        app:layout_constraintTop_toBottomOf="@+id/miyaButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/nisihosoeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:background="#BB8BFF"
        android:fontFamily="serif"
        android:text="西細江"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/kawatigosoeButton"
        app:layout_constraintTop_toBottomOf="@+id/tensinButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/nakahosoeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:background="#FFAB00"
        android:fontFamily="serif"
        android:text="中細江"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/oohamaButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/minatoButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:background="#A3F3FF"
        android:fontFamily="serif"
        android:text="港"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/nakahosoeButton"
        app:layout_constraintTop_toBottomOf="@+id/kawatigosoeButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/minamihosoeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:background="#FFD600"
        android:fontFamily="serif"
        android:text="南細江"
        app:layout_constraintStart_toEndOf="@+id/minatoButton"
        app:layout_constraintTop_toBottomOf="@+id/nisihosoeButton" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="33dp"
        android:fontFamily="sans-serif-black"
        android:text="見どころ"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/minatoButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/hamanomiyaMidokoro1Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="15dp"
        android:text="見どころ1：場所を確認"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/hamanomiyaMidokoro2Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="30dp"
        android:text="見どころ2"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/hamanomiyaMidokoro3Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="30dp"
        android:text="見どころ3"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="10dp"
        android:text="10月8日正午ごろ台場差し"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hamanomiyaMidokoro1Button" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="10dp"
        android:text="10月9日午前10時ごろ・午後6時ごろ台場差し"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hamanomiyaMidokoro2Button" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="10dp"
        android:text="10月9日午後2時ごろ宮公園台場差し"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hamanomiyaMidokoro3Button" />
</androidx.constraintlayout.widget.ConstraintLayout>