<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4EED0"
    tools:context=".Hamanomiya">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44sp"
        android:layout_marginBottom="41dp"
        android:fontFamily="serif"
        android:text="屋台蔵の場所を確認する"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/miyaButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/sukaButton"
        android:layout_width="107dp"
        android:layout_height="48dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/hamanomiyaback"
        android:fontFamily="casual"
        android:text="須加:すか"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/miyaButton"
        android:layout_width="107dp"
        android:layout_height="48dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="34dp"
        android:background="@drawable/hamanomiyaback"
        android:fontFamily="casual"
        android:text="宮:みや"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/tensinButton"
        app:layout_constraintStart_toEndOf="@+id/sukaButton"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/tensinButton"
        android:layout_width="107dp"
        android:layout_height="48dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="45dp"
        android:background="@drawable/hamanomiyaback"
        android:fontFamily="casual"
        android:text="天神:てんじん"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/oohamaButton"
        android:layout_width="107dp"
        android:layout_height="48dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/hamanomiyaback"
        android:fontFamily="casual"
        android:text="大浜:おおはま"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sukaButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/kawatigosoeButton"
        android:layout_width="107dp"
        android:layout_height="48dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="34dp"
        android:background="@drawable/hamanomiyaback"
        android:fontFamily="casual"
        android:text="川内細江:かわちほそえ"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/nisihosoeButton"
        app:layout_constraintStart_toEndOf="@+id/oohamaButton"
        app:layout_constraintTop_toBottomOf="@+id/miyaButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/nisihosoeButton"
        android:layout_width="107dp"
        android:layout_height="48dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="45dp"
        android:background="@drawable/hamanomiyaback"
        android:fontFamily="casual"
        android:text="西細江:にしほそえ"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tensinButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/nakahosoeButton"
        android:layout_width="107dp"
        android:layout_height="48dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/hamanomiyaback"
        android:fontFamily="casual"
        android:text="中細江:なかほそえ"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/oohamaButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/minatoButton"
        android:layout_width="107dp"
        android:layout_height="48dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="34dp"
        android:background="@drawable/hamanomiyaback"
        android:fontFamily="casual"
        android:text="港:みなと"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/minamihosoeButton"
        app:layout_constraintStart_toEndOf="@+id/nakahosoeButton"
        app:layout_constraintTop_toBottomOf="@+id/kawatigosoeButton" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/minamihosoeButton"
        android:layout_width="107dp"
        android:layout_height="48dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="45dp"
        android:background="@drawable/hamanomiyaback"
        android:fontFamily="casual"
        android:text="南細江:みなみほそえ"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nisihosoeButton" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="33sp"
        android:fontFamily="serif"
        android:text="見所"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/minatoButton" />

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="370dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/hamanomiyaMidokoro1Button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="34dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/rootbutton"
                android:fontFamily="serif"
                android:text="見所1：場所を確認"
                android:textColor="#000000"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView14" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="233dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="34sp"
                android:layout_marginTop="10sp"
                android:fontFamily="serif"
                android:text="10月8日正午ごろ台場差し"
                android:textColor="#000000"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/hamanomiyaMidokoro1Button" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/hamanomiyaMidokoro2Button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="34dp"
                android:layout_marginTop="30dp"
                android:background="@drawable/rootbutton"
                android:fontFamily="serif"
                android:text="見所2:場所を確認"
                android:textColor="#000000"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView15" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="373dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="34dp"
                android:layout_marginTop="10dp"
                android:fontFamily="serif"
                android:text="10月9日午前10時ごろ・午後6時ごろ台場差し"
                android:textColor="#000000"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/hamanomiyaMidokoro2Button" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/hamanomiyaMidokoro3Button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="34dp"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/rootbutton"
                android:fontFamily="serif"
                android:text="見所3:場所を確認"
                android:textColor="#000000"
                android:textSize="16sp"
                app:layout_constraintBottom_toTopOf="@+id/textView17"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView16" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="315dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="34dp"
                android:layout_marginBottom="84dp"
                android:fontFamily="serif"
                android:text="10月9日午後2時ごろ宮公園台場差し"
                android:textColor="#000000"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>